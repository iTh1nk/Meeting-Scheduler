{
  "name": "server",
  "version": "1.0.0",
  "description": "Meeting scheduler online management",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "deploy": "cd client && npm run build && serve -p 3009 -s build",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iTh1nk/Meeting-Scheduler.git"
  },
  "engines": {
    "node": "12.x"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iTh1nk/Meeting-Scheduler/issues"
  },
  "homepage": "https://github.com/iTh1nk/Meeting-Scheduler#readme",
  "dependencies": {
    "antd": "^4.3.5",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bootstrap": "^4.5.0",
    "colors": "^1.4.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "mongoose": "^5.9.19",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "react-router-dom": "^5.2.0",
    "socket.io": "^2.3.0"
  }
}
